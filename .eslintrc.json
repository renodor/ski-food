{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", {
          "code": 120,
          "ignoreUrls": true
        }],
        "comma-dangle": ["error", "never"]
    }
}
